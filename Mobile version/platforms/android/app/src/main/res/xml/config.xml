<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ANTHONYM.passwordwarrior" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Toast">
        <param name="android-package" value="nl.xservices.plugins.Toast" />
    </feature>
    <name>Password Warrior</name>
    <description>
        Password managing application
    </description>
    <author email="samuelmatheson15@gmail.com" href="mailto:samuelmatheson15@gmail.com">
        Samuel A. Matheson
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:18765744891" />
    <allow-intent href="sms:18765744801" />
    <allow-intent href="mailto:samuelmatheson15@gmail.com" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon src="res/icon/icon.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
